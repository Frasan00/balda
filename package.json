{
  "name": "balda",
  "version": "0.0.39",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "repository": "git@github.com:Frasan00/balda.git",
  "author": "Frasan00 <francesco.sangiovanni.7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test:cron": "tsx test/cron/index.ts",
    "test:cron:bun": "bun run --watch test/cron/index.ts",
    "test:cron:deno": "deno task test:cron",
    "benchmark": "bash run-benchmarks.sh --quick",
    "benchmark:all": "bash run-benchmarks.sh --all",
    "benchmark:runtime": "bash run-benchmarks.sh --runtime",
    "benchmark:plugins": "bash run-benchmarks.sh --plugins",
    "benchmark:quick": "tsx test/benchmark/quick-bench.ts",
    "benchmark:scenarios": "tsx test/benchmark/all-scenarios.ts",
    "benchmark:old": "tsx test/benchmark/index.ts",
    "benchmark:bun": "USE_BUN=1 bash run-benchmarks.sh --quick",
    "benchmark:deno": "USE_DENO=1 bash run-benchmarks.sh --quick",
    "dev": "tsx watch test/server/index.ts",
    "dev:bun": "bun run --watch test/server/index.ts",
    "dev:deno": "deno task dev",
    "build": "rm -rf lib && docker compose exec -T node yarn tsup",
    "build:test": "rm -rf lib && docker compose exec -T node yarn tsup && rm -rf lib",
    "build:prod": "rm -rf lib && docker compose exec -T node yarn tsup --minify",
    "format": "docker compose exec -T node yarn prettier --write \"**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "docker compose exec -T node yarn vitest --run",
    "test:bun": "docker compose exec -T bun bun test",
    "test:deno": "docker compose exec -T deno deno task test",
    "test:all": "docker compose exec -T node yarn vitest --run && docker compose exec -T bun bun test && docker compose exec -T deno deno task test",
    "test:watch": "docker compose exec -T node yarn vitest --watch",
    "test:coverage": "docker compose exec -T node yarn vitest --coverage",
    "prepare": "husky"
  },
