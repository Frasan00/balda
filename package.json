{
  "name": "balda-js",
  "version": "0.0.46",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "repository": "git@github.com:Frasan00/balda-js.git",
  "author": "Frasan00 <francesco.sangiovanni.7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test:cron": "tsx test/cron/index.ts",
    "test:cron:bun": "bun run --watch test/cron/index.ts",
    "test:cron:deno": "deno task test:cron",
    "benchmark": "tsx test/benchmark/index.ts",
    "benchmark:bun": "bun run --watch test/benchmark/index.ts",
    "benchmark:deno": "deno task benchmark",
    "dev": "tsx watch test/server/index.ts",
    "dev:bun": "bun run --watch test/server/index.ts",
    "dev:deno": "deno task dev",
    "build": "rm -rf lib && tsup",
    "build:test": "rm -rf lib && tsup && rm -rf lib",
    "build:prod": "rm -rf lib && tsup --minify",
    "format": "prettier --write \"**/*.ts\"",
    "test": "vitest --run",
    "test:bun": "bun test",
    "test:deno": "deno task test",
    "test:all": "yarn test && yarn test:bun && yarn test:deno",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepare": "husky",
    "docs:dev": "cd docs && yarn start",
    "docs:build": "cd docs && yarn build",
    "docs:serve": "cd docs && yarn serve"
  },
  "devDependencies": {
    "@aws-sdk/client-sqs": "^3.911.0",
    "@types/bun": "^1.2.17",
    "@types/deno": "^2.3.0",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "autocannon": "^8.0.0",
    "bullmq": "^5.61.0",
    "bun-types": "^1.2.17",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "ioredis": "^5.8.1",
    "lint-staged": "^16.1.2",
    "node-cron": "^4.2.1",
    "node-gyp": "^11.2.0",
    "pg": "^8.16.3",
    "pg-boss": "^11.1.1",
    "prettier": "^3.6.2",
    "socket.io": "^4.8.1",
    "sqs-consumer": "^14.0.1",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "ws": "^8.18.2"
  },
  "bin": {
    "balda": "lib/cli.js"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "glob": "^11.0.3",
    "pino": "^10.0.0",
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "bullmq": "^5.61.0",
    "ioredis": "^5.8.1",
    "node-cron": "^4.1.0",
    "pg": "^8.16.3",
    "pg-boss": "^11.1.1",
    "sqs-consumer": "^14.0.1"
  },
  "tags": [
    "framework",
    "http",
    "server",
    "node",
    "bun",
    "deno",
    "backend"
  ]
}
