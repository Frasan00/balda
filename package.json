{
  "name": "balda",
  "version": "0.0.03",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "repository": "git@github.com:Frasan00/balda.git",
  "author": "Frasan00 <francesco.sangiovanni.7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test:cron": "tsx test/cron/index.ts",
    "test:cron:bun": "bun run --watch test/cron/index.ts",
    "test:cron:deno": "deno task test:cron",
    "benchmark": "tsx test/benchmark/index.ts",
    "benchmark:bun": "bun run --watch test/benchmark/index.ts",
    "benchmark:deno": "deno task benchmark",
    "dev": "tsx watch test/server/index.ts",
    "dev:bun": "bun run --watch test/server/index.ts",
    "dev:deno": "deno task dev",
    "build": "rm -rf lib && tsup",
    "build:test": "rm -rf lib && tsup && rm -rf lib",
    "build:prod": "rm -rf lib && tsup --minify",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --run",
    "test:bun": "bun test",
    "test:deno": "deno task test",
    "test:all": "yarn test && yarn test:bun && yarn test:deno",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@adminjs/express": "^6.1.1",
    "@aws-sdk/client-s3": "^3.956.0",
    "@aws-sdk/client-sqs": "^3.956.0",
    "@aws-sdk/cloudfront-signer": "^3.956.0",
    "@aws-sdk/s3-presigned-post": "^3.956.0",
    "@aws-sdk/s3-request-presigner": "^3.956.0",
    "@azure/storage-blob": "^12.29.1",
    "@types/bun": "^1.3.5",
    "@types/deno": "^2.5.0",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "adminjs": "^7.8.17",
    "autocannon": "^8.0.0",
    "bullmq": "^5.66.2",
    "esbuild": "^0.27.2",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.39.2",
    "express": "^5.2.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.2",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.18.0",
    "husky": "^9.1.7",
    "ioredis": "^5.8.2",
    "mqtt": "^5.14.1",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pg-boss": "^12.5.3",
    "prettier": "^3.7.4",
    "sqs-consumer": "^14.2.0",
    "swagger-typescript-api": "^13.2.16",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "ws": "^8.18.3",
    "zod": "^4.2.1"
  },
  "bin": {
    "balda": "lib/cli.js"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "glob": "^13.0.0",
    "pino": "^10.1.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.956.0",
    "@aws-sdk/client-sqs": "^3.956.0",
    "@aws-sdk/cloudfront-signer": "^3.956.0",
    "@aws-sdk/s3-presigned-post": "^3.956.0",
    "@aws-sdk/s3-request-presigner": "^3.956.0",
    "@azure/storage-blob": "^12.29.1",
    "bullmq": "^5.66.2",
    "esbuild": "^0.27.2",
    "esbuild-plugin-copy": "^2.1.1",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.18.0",
    "ioredis": "^5.8.2",
    "mqtt": "^5.14.1",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pg-boss": "^12.5.3",
    "sqs-consumer": "^14.2.0",
    "zod": "^4.0.0",
    "swagger-typescript-api": "^13.2.16"
  },
  "engines": {
    "node": ">=18.20.8",
    "bun": ">=1.3.5",
    "deno": ">=2.5.0"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always"
  },
  "tags": [
    "framework",
    "http",
    "server",
    "node",
    "bun",
    "deno",
    "backend"
  ]
}
